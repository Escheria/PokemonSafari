{
        "manifest_version": 2,
        "name": "Pokemon Safari",
        "version": "1.5",
        "description": "Makes wild Pokemon appear in your browser. Find them, capture them, and store them into your Pokedex.",
        "permissions": ["<all_urls>", "storage", "alarms", "notifications"],
        "browser_action": {
            "default_icon": "iconPokeball.png",
            "default_popup": "menu.html"
        },
        "background": {
            "scripts": ["encounter.js"],
            "persistent": false
        },
        "options_page": "options.html",
        "icons": {
            "128": "safari128.png",
            "48": "iconPokeball.png"
        }
}