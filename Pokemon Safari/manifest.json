{
        "manifest_version": 2,
        "name": "Pokemon Safari",
        "version": "1.0",
        "description": "Pokeparty.",
        "permissions": ["<all_urls>", "storage", "alarms"],
        "browser_action": {
            "default_icon": "iconPokeball.png",
            "default_popup": "menu.html"
        },
        "background": {
            "scripts": ["encounter.js"],
            "persistent": false
        },
        "icons": {
            "128": "safari128.png",
            "48": "iconPokeball.png"
        }
}